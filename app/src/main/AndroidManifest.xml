<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yeni">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:testOnly="false">

        <activity android:name=".OrderStatus"
            android:label="Order Status"
            />
        <activity android:name=".Cart"
            android:label="Cart"
            />
        <activity android:name=".ProductDetail"
            android:label="Product Detail"/>

        <activity android:name=".ProductList"
            android:label="Product List"/>
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".signUpPage"
            android:label="Sign Up"/>
        <activity android:name=".signInPage"
            android:label="Sign In"/>
        <activity android:label="Bayraktar Market" android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>